<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.*?>
<GridPane alignment="center" hgap="10" prefHeight="378.0" prefWidth="600.0" vgap="10" xmlns="http://javafx.com/javafx/8"
          xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.Controller">
    <columnConstraints>
        <ColumnConstraints maxWidth="470.0" minWidth="370.0" prefWidth="370.0"/>
        <ColumnConstraints maxWidth="141.0" minWidth="41.0" prefWidth="141.0"/>
    </columnConstraints>
    <rowConstraints>
        <RowConstraints maxHeight="15.0" minHeight="4.0" prefHeight="15.0"/>
        <RowConstraints maxHeight="118.0" minHeight="17.0" prefHeight="118.0"/>
        <RowConstraints maxHeight="189.0" minHeight="89.0" prefHeight="89.0"/>
        <RowConstraints maxHeight="0.0" minHeight="0.0" prefHeight="0.0"/>
        <RowConstraints/>
        <RowConstraints/>
    </rowConstraints>
    <children>
        <Button fx:id="search" mnemonicParsing="false" prefHeight="16.0" prefWidth="239.0" text="wyszukaj"
                GridPane.columnIndex="1"/>
        <TextField fx:id="query" promptText="czego szukasz czego pragniesz?"/>
        <TextArea fx:id="result" prefHeight="131.0" prefWidth="370.0" promptText="się to wypełni - Yoda"
                  GridPane.rowIndex="2"/>
        <CheckBox fx:id="showSuggestions" mnemonicParsing="false" prefHeight="16.0" prefWidth="234.0" text="podpowiedzi"
                  GridPane.columnIndex="1" GridPane.rowIndex="1"/>

        <VBox prefHeight="137.0" prefWidth="370.0" GridPane.rowIndex="1" fx:id="linkList">

        </VBox>
    </children>
</GridPane>
